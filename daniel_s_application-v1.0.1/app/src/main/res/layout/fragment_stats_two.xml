                    android:id="@+id/recyclerStatsTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                    app:spanCount="10" itemSpace="@{@dimen/_1pxh}"
                    style="@style/groupStylegray_50"
                    tools:itemCount="35" tools:listitem="@layout/row_two"/>
                <View android:background="@color/indigo_900_70"
                    android:id="@+id/lineLinenineOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_59pxv" android:layout_width="@dimen/_357pxh"/>
                <FrameLayout android:id="@+id/frameStackmonth"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_29pxv"
                    android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_102pxh">
                    <LinearLayout android:id="@+id/linearColumnmonth"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_14pxh"
                        android:paddingStart="@dimen/_14pxh"
                        android:paddingTop="@dimen/_4pxv" style="@style/groupStyleindigo_A700cornerRadius">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMonth"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="match_parent"
