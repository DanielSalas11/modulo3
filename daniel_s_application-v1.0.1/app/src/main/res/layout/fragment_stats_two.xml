                            android:text="@{statsTwoVM.statsTwoModel.txtTwo}"
                            style="@style/txtMontserratromansemibold12" tools:text="@string/lbl_2"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnthree"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearRow"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageImagethreeOne"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_40pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_38pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_image_3"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_image_3"/>
                            <ImageView
                                android:id="@+id/imageImagefourOne"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_40pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_40pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_image_4"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_image_4"/>
                            <ImageView
                                android:id="@+id/imageImagefiveOne"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_40pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_image_4"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_image_4"/>
