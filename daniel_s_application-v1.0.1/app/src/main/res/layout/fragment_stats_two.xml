                                    android:id="@+id/imageImagesixOne"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_21pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_image_1"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_1"/>
                                <ImageView
                                    android:id="@+id/imageImageeightOne"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_image_3"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_3"/>
                            </LinearLayout>
