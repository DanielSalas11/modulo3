                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_54pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:paddingHorizontal="@dimen/_7pxh"
                        android:paddingVertical="@dimen/_7pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_arrow_left_white_a700" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <View android:background="@color/indigo_900_70"
                    android:id="@+id/lineLinesevenOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_359pxh"/>
                <View android:background="@color/indigo_900_70"
                    android:id="@+id/lineLineeightOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_359pxh"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowmon"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMon"
                        android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{statsTwoVM.statsTwoModel.txtMon}"
                        style="@style/txtMontserratromansemibold14" tools:text="@string/lbl_mon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTue"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{statsTwoVM.statsTwoModel.txtTue}"
                        style="@style/txtMontserratromansemibold14" tools:text="@string/lbl_tue"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWed"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{statsTwoVM.statsTwoModel.txtWed}"
