                            android:text="@{statsTwoVM.statsTwoModel.txtMonth}"
                            style="@style/txtMontserratromanregular14_2" tools:text="@string/lbl_month"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnCalendario"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_5pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_5pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_calendario"
                        android:textAllCaps="false"
                        style="@style/btnSolid_1" tools:text="@string/lbl_calendario"/>
