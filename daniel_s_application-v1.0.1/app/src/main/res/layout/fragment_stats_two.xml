                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{statsTwoVM.statsTwoModel.txtFive}"
                                style="@style/txtMontserratromansemibold12" tools:text="@string/lbl_5"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSix"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_42pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{statsTwoVM.statsTwoModel.txtSix}"
                                style="@style/txtMontserratromansemibold12" tools:text="@string/lbl_6"/>
