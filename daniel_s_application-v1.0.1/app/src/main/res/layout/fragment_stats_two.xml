                                android:id="@+id/linearColumnOne"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <ImageView
                                    android:id="@+id/imageImagesevenOne"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_image_2"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_2"/>
                                <ImageView
                                    android:id="@+id/imageImagefourThree"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_19pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_image_4"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_4"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearRowthree"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtThree"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{statsTwoVM.statsTwoModel.txtThree}"
                                style="@style/txtMontserratromansemibold12" tools:text="@string/lbl_3"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFour"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_36pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{statsTwoVM.statsTwoModel.txtFour}"
                                style="@style/txtMontserratromansemibold12" tools:text="@string/lbl_4"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFive"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
